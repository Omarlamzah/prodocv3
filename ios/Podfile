# Uncomment this line to define a global platform for your project
platform :ios, '15.5'

# CocoaPods analytics sends network stats synchronously affecting flutter build latency.
ENV['COCOAPODS_DISABLE_STATS'] = 'true'

project 'Runner', {
  'Debug' => :debug,
  'Profile' => :release,
  'Release' => :release,
}

def flutter_root
  generated_xcode_build_settings_path = File.expand_path(File.join('..', 'Flutter', 'Generated.xcconfig'), __FILE__)
  unless File.exist?(generated_xcode_build_settings_path)
    raise "#{generated_xcode_build_settings_path} must exist. If you're running pod install manually, make sure flutter pub get is executed first"
  end

  File.foreach(generated_xcode_build_settings_path) do |line|
    matches = line.match(/FLUTTER_ROOT\=(.*)/)
    return matches[1].strip if matches
  end
  raise "FLUTTER_ROOT not found in #{generated_xcode_build_settings_path}. Try deleting Generated.xcconfig, then run flutter pub get"
end

require File.expand_path(File.join('packages', 'flutter_tools', 'bin', 'podhelper'), flutter_root)

flutter_ios_podfile_setup

target 'Runner' do
  use_frameworks!
  use_modular_headers!

  flutter_install_all_ios_pods File.dirname(File.realpath(__FILE__))
end

post_install do |installer|
  installer.pods_project.targets.each do |target|
    flutter_additional_ios_build_settings(target)
    target.build_configurations.each do |config|
      config.build_settings['IPHONEOS_DEPLOYMENT_TARGET'] = '15.5'
      # Fix for Swift Package Manager dependencies
      config.build_settings['BUILD_LIBRARY_FOR_DISTRIBUTION'] = 'YES'
      config.build_settings['SWIFT_VERSION'] = '5.0'
      # Enable Swift Package Manager support
      config.build_settings['ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES'] = 'YES'
    end
    
    # Comprehensive fix for "Build input file cannot be found" error with privacy bundles
    # Privacy bundles are resource-only and should not be code signed
    target_name = target.name.downcase
    is_privacy_bundle = target_name.include?('privacy') || 
                       target_name.end_with?('_privacy') ||
                       target_name.end_with?('_privacy.bundle')
    
    # Also check product name and wrapper name
    product_name = target.respond_to?(:product_name) ? target.product_name.to_s.downcase : ''
    wrapper_name = target.respond_to?(:wrapper_name) ? target.wrapper_name.to_s.downcase : ''
    is_privacy_bundle = is_privacy_bundle || 
                       product_name.include?('privacy') || 
                       wrapper_name.include?('privacy')
    
    if is_privacy_bundle || (target.respond_to?(:product_type) && target.product_type == 'com.apple.product-type.bundle')
      puts "Processing privacy bundle target: #{target.name} (is_privacy_bundle: #{is_privacy_bundle})"
      target.build_configurations.each do |config|
        config.build_settings['CODE_SIGNING_ALLOWED'] = 'NO'
        config.build_settings['CODE_SIGNING_REQUIRED'] = 'NO'
        config.build_settings['CODE_SIGNING_INJECT_BASE_ENTITLEMENTS'] = 'NO'
        config.build_settings['MACH_O_TYPE'] = 'mh_bundle'
        config.build_settings['SKIP_INSTALL'] = 'YES'
        config.build_settings['STRIP_INSTALLED_PRODUCT'] = 'NO'
        # Prevent Xcode from looking for a binary in resource-only bundles
        config.build_settings['EXECUTABLE_EXTENSION'] = ''
        config.build_settings['EXECUTABLE_PREFIX'] = ''
        # Ensure the bundle is treated as a resource bundle
        config.build_settings['PRODUCT_TYPE'] = 'com.apple.product-type.bundle'
        # Allow missing input files (the script phase will create them)
        config.build_settings['ALWAYS_SEARCH_USER_PATHS'] = 'NO'
      end
      
      # Remove any file references that might cause Xcode to look for the privacy manifest as a build input
      # Also remove any output paths that might conflict with our script phase
      target.build_phases.each do |phase|
        # Remove file references from Copy Bundle Resources and other phases
        if phase.respond_to?(:files)
          files_to_remove = []
          phase.files.to_a.each do |file|
            if file.respond_to?(:file_ref) && file.file_ref
              begin
                file_name = file.file_ref.path rescue nil
                if file_name && (file_name.include?('privacy') || file_name.end_with?('_privacy'))
                  files_to_remove << file
                end
              rescue
                # Ignore errors
              end
            end
          end
          files_to_remove.each { |f| phase.files.delete(f) rescue nil }
        end
        
        # Remove output paths that might conflict
        if phase.respond_to?(:output_paths)
          phase.output_paths = phase.output_paths.reject { |path| path.to_s.include?('privacy') } rescue nil
        end
        
        if phase.respond_to?(:output_file_list_paths)
          phase.output_file_list_paths = phase.output_file_list_paths.reject { |path| path.to_s.include?('privacy') } rescue nil
        end
        
        # Also check input file lists
        if phase.respond_to?(:input_file_list_paths)
          phase.input_file_list_paths = phase.input_file_list_paths.reject { |path| path.to_s.include?('privacy') } rescue nil
        end
        
        # Check input paths
        if phase.respond_to?(:input_paths)
          phase.input_paths = phase.input_paths.reject { |path| path.to_s.include?('privacy') } rescue nil
        end
      end
      
      # Add script phase to create the privacy manifest file during build
      # The file name should match the target name (e.g., url_launcher_ios_privacy)
      script_phase_name = 'Create Privacy Manifest File'
      # Remove any existing script phase with this name first
      existing_scripts = target.shell_script_build_phases.select { |phase| phase.name == script_phase_name }
      existing_scripts.each { |phase| target.build_phases.delete(phase) rescue nil }
      
      # Create new script phase
      script_phase = target.new_shell_script_build_phase(script_phase_name)
      
      # Extract package name from target name
      package_name = target.name.gsub(/_privacy$/i, '').gsub(/_Privacy$/i, '')
      
      # Declare the output path - CRITICAL: This tells Xcode the script creates this file
      # The path format must match exactly what Xcode expects as a build input
      # Format: ${UNINSTALLED_PRODUCTS_DIR}/${WRAPPER_NAME}/${TARGET_NAME}
      # For privacy bundles: ${UNINSTALLED_PRODUCTS_DIR}/url_launcher_ios_privacy.bundle/url_launcher_ios_privacy
      # Use both UNINSTALLED_PRODUCTS_DIR and BUILT_PRODUCTS_DIR to cover all build scenarios
      output_paths = [
        "${UNINSTALLED_PRODUCTS_DIR}/${WRAPPER_NAME}/${TARGET_NAME}",
        "${BUILT_PRODUCTS_DIR}/${WRAPPER_NAME}/${TARGET_NAME}"
      ]
      
      script_phase.shell_script = <<-SCRIPT
#!/bin/bash
set -euo pipefail

# Create privacy manifest file - runs before Xcode checks for build inputs
TARGET="${TARGET_NAME}"
WRAPPER="${WRAPPER_NAME}"

echo "[Privacy] Creating manifest for ${TARGET} in ${WRAPPER}"

# Create file in UNINSTALLED_PRODUCTS_DIR (archive builds - this is where the error occurs)
if [ -n "${UNINSTALLED_PRODUCTS_DIR:-}" ]; then
  ARCHIVE_DIR="${UNINSTALLED_PRODUCTS_DIR}/${WRAPPER}"
  ARCHIVE_FILE="${ARCHIVE_DIR}/${TARGET}"
  
  # Force create directory and file
  mkdir -p "${ARCHIVE_DIR}"
  touch "${ARCHIVE_FILE}"
  chmod 644 "${ARCHIVE_FILE}"
  
  if [ -f "${ARCHIVE_FILE}" ]; then
    echo "[Privacy] ✓ Created: ${ARCHIVE_FILE}"
  else
    echo "[Privacy] ✗ FAILED to create: ${ARCHIVE_FILE}"
    exit 1
  fi
fi

# Also create in BUILT_PRODUCTS_DIR (regular builds)
if [ -n "${BUILT_PRODUCTS_DIR:-}" ]; then
  REGULAR_DIR="${BUILT_PRODUCTS_DIR}/${WRAPPER}"
  REGULAR_FILE="${REGULAR_DIR}/${TARGET}"
  
  mkdir -p "${REGULAR_DIR}"
  touch "${REGULAR_FILE}"
  chmod 644 "${REGULAR_FILE}"
  
  if [ -f "${REGULAR_FILE}" ]; then
    echo "[Privacy] ✓ Created: ${REGULAR_FILE}"
  fi
fi

echo "[Privacy] Completed for ${TARGET}"
        SCRIPT
      
      # CRITICAL: Declare output paths so Xcode knows this script creates the files
      # This must be set as an array
      script_phase.output_paths = output_paths
      script_phase.run_only_for_deployment_postprocessing = '0'
      script_phase.always_out_of_date = '1'
      # Ensure no inputs are declared
      script_phase.input_file_list_paths = [] rescue nil
      script_phase.input_paths = [] rescue nil
      
      # CRITICAL: Move script phase to the VERY BEGINNING to run before Xcode checks for inputs
      # This must run before any Copy Bundle Resources or other phases
      begin
        # Get all build phases
        all_phases = target.build_phases.to_a
        
        # Remove script phase if it exists
        all_phases.delete(script_phase)
        
        # Insert at the very beginning
        all_phases.unshift(script_phase)
        
        # Rebuild phases array
        target.build_phases.clear
        all_phases.each { |p| target.build_phases << p }
        
        position = target.build_phases.index(script_phase)
        puts "  ✓ Script phase moved to beginning for #{target.name} (position: #{position || 'unknown'})"
        puts "  ✓ Output path declared: ${UNINSTALLED_PRODUCTS_DIR}/${WRAPPER_NAME}/${TARGET_NAME}"
      rescue => e
        puts "  ⚠ Could not reorder build phases for #{target.name}: #{e.message}"
        # Fallback: try to add at beginning
        begin
          unless target.build_phases.to_a.include?(script_phase)
            target.build_phases.unshift(script_phase)
            puts "  ✓ Added script phase to beginning for #{target.name}"
          end
        rescue => e2
          puts "  ✗ Failed to add script phase for #{target.name}: #{e2.message}"
        end
      end
    end
  end
  
  # Ensure Swift Package Manager dependencies are resolved and disable sandboxing
  installer.pods_project.build_configurations.each do |config|
    config.build_settings['BUILD_LIBRARY_FOR_DISTRIBUTION'] = 'YES'
    config.build_settings['ENABLE_USER_SCRIPT_SANDBOXING'] = 'NO'
  end
  
  # Add script phase to Runner target to create privacy manifests BEFORE build
  # This is critical because Xcode checks for these files before pod targets are built
  begin
    require 'xcodeproj'
    project_path = File.join(File.dirname(__FILE__), 'Runner.xcodeproj')
    if File.exist?(project_path)
      project = Xcodeproj::Project.open(project_path)
      runner_target = project.targets.find { |t| t.name == 'Runner' }
      
      if runner_target
      script_phase_name = 'Create Privacy Manifests'
      # Remove existing script phase if it exists
      existing_phase = runner_target.shell_script_build_phases.find { |p| p.name == script_phase_name }
      runner_target.build_phases.delete(existing_phase) if existing_phase
      
      # Create new script phase
      script_phase = runner_target.new_shell_script_build_phase(script_phase_name)
      script_phase.shell_script = <<-SCRIPT
#!/bin/bash
set -e

# Create privacy manifest files for all packages that need them
# This MUST run before Xcode checks for these files as build inputs
PACKAGES="url_launcher_ios sqflite_darwin shared_preferences_foundation path_provider_foundation flutter_secure_storage flutter_local_notifications share_plus"

create_manifest() {
  local pkg=$1
  local base_dir=$2
  
  if [ -z "$pkg" ] || [ -z "$base_dir" ]; then
    return 0
  fi
  
  BUNDLE_DIR="${base_dir}/${pkg}_privacy.bundle"
  mkdir -p "$BUNDLE_DIR" || true
  touch "${BUNDLE_DIR}/${pkg}_privacy" || true
  chmod 644 "${BUNDLE_DIR}/${pkg}_privacy" || true
  echo "[Privacy] Created: ${BUNDLE_DIR}/${pkg}_privacy"
  return 0
}

# Create in UNINSTALLED_PRODUCTS_DIR (for archive builds - this is where the error occurs)
if [ -n "${UNINSTALLED_PRODUCTS_DIR:-}" ]; then
  echo "[Privacy] Creating manifests in UNINSTALLED_PRODUCTS_DIR: ${UNINSTALLED_PRODUCTS_DIR}"
  for pkg in $PACKAGES; do
    create_manifest "$pkg" "${UNINSTALLED_PRODUCTS_DIR}"
  done
fi

# Create in BUILT_PRODUCTS_DIR (for regular builds)
if [ -n "${BUILT_PRODUCTS_DIR:-}" ]; then
  echo "[Privacy] Creating manifests in BUILT_PRODUCTS_DIR: ${BUILT_PRODUCTS_DIR}"
  for pkg in $PACKAGES; do
    create_manifest "$pkg" "${BUILT_PRODUCTS_DIR}"
  done
fi

# Also create in DerivedData locations (fallback for archive builds)
DERIVED_DATA_BASE="$HOME/Library/Developer/Xcode/DerivedData"
if [ -d "$DERIVED_DATA_BASE" ]; then
  echo "[Privacy] Checking DerivedData: $DERIVED_DATA_BASE"
  for runner_dir in "$DERIVED_DATA_BASE"/Runner-*; do
    if [ -d "$runner_dir" ]; then
      uninstalled_dir="${runner_dir}/Build/Intermediates.noindex/ArchiveIntermediates/Runner/IntermediateBuildFilesPath/UninstalledProducts/iphoneos"
      parent_dir="$(dirname "$uninstalled_dir")"
      if [ -d "$parent_dir" ] || mkdir -p "$parent_dir" 2>/dev/null; then
        mkdir -p "$uninstalled_dir" || true
        echo "[Privacy] Creating manifests in: $uninstalled_dir"
        for pkg in $PACKAGES; do
          create_manifest "$pkg" "$uninstalled_dir"
        done
      fi
    fi
  done
fi

echo "[Privacy] Script phase completed"
      SCRIPT
      
      # Declare output paths so Xcode knows these files are created
      output_paths = []
      ['url_launcher_ios', 'sqflite_darwin', 'shared_preferences_foundation', 
       'path_provider_foundation', 'flutter_secure_storage', 
       'flutter_local_notifications', 'share_plus'].each do |pkg|
        output_paths << "${UNINSTALLED_PRODUCTS_DIR}/${pkg}_privacy.bundle/${pkg}_privacy"
        output_paths << "${BUILT_PRODUCTS_DIR}/${pkg}_privacy.bundle/${pkg}_privacy"
      end
      script_phase.output_paths = output_paths
      script_phase.run_only_for_deployment_postprocessing = '0'
      script_phase.always_out_of_date = '1'
      
      # Move script phase to the VERY BEGINNING (before Flutter build script)
      all_phases = runner_target.build_phases.to_a
      all_phases.delete(script_phase)
      all_phases.unshift(script_phase)
      runner_target.build_phases.clear
      all_phases.each { |p| runner_target.build_phases << p }
      
      project.save
      
      # Verify the script phase was added
      runner_target = project.targets.find { |t| t.name == 'Runner' }
      if runner_target
        added_phase = runner_target.shell_script_build_phases.find { |p| p.name == script_phase_name }
        if added_phase
          position = runner_target.build_phases.index(added_phase)
          puts "✓ Added 'Create Privacy Manifests' script phase to Runner target"
          puts "  Position in build phases: #{position || 'unknown'}"
          puts "  Output paths declared: #{added_phase.output_paths.length} paths"
        else
          puts "⚠ Script phase was not found after adding (this is unexpected)"
        end
      end
      else
        puts "⚠ Runner target not found in Xcode project"
      end
    else
      puts "⚠ Runner.xcodeproj not found"
    end
  rescue LoadError => e
    puts "⚠ xcodeproj gem not available: #{e.message}"
    puts "  Install with: gem install xcodeproj"
    puts "  Continuing without script phase modification..."
  rescue => e
    puts "⚠ Failed to add script phase to Runner target: #{e.message}"
    puts "  #{e.backtrace.first}"
    puts "  Continuing - files will be created via other methods..."
  end
  
  # Create placeholder privacy manifest files after pod install
  # This ensures they exist before Xcode checks for them as build inputs
  require 'fileutils'
  puts "Creating placeholder privacy manifest files..."
  privacy_packages = ['url_launcher_ios', 'sqflite_darwin', 'shared_preferences_foundation', 
                      'path_provider_foundation', 'flutter_secure_storage', 
                      'flutter_local_notifications', 'share_plus']
  
  # Create in build directory
  build_dir = File.join(File.dirname(__FILE__), '..', 'build', 'ios', 'Release-iphoneos')
  FileUtils.mkdir_p(build_dir) rescue nil
  
  privacy_packages.each do |pkg|
    bundle_dir = File.join(build_dir, "#{pkg}_privacy.bundle")
    FileUtils.mkdir_p(bundle_dir) rescue nil
    manifest_file = File.join(bundle_dir, "#{pkg}_privacy")
    FileUtils.touch(manifest_file) rescue nil
    FileUtils.chmod(0644, manifest_file) rescue nil
    puts "  Created placeholder: #{manifest_file}"
  end
  
  # Also create in Pods directory structure so they're part of the project
  pods_dir = File.join(File.dirname(__FILE__), 'Pods')
  if File.directory?(pods_dir)
    privacy_packages.each do |pkg|
      # Create privacy bundle in Pods directory
      pod_bundle_dir = File.join(pods_dir, "#{pkg}_privacy.bundle")
      FileUtils.mkdir_p(pod_bundle_dir) rescue nil
      pod_manifest_file = File.join(pod_bundle_dir, "#{pkg}_privacy")
      FileUtils.touch(pod_manifest_file) rescue nil
      FileUtils.chmod(0644, pod_manifest_file) rescue nil
      puts "  Created in Pods: #{pod_manifest_file}"
    end
  end
  
  # CRITICAL: Create files in DerivedData locations that Xcode will check
  # This must happen after pod install but before any build
  # Create files in ALL possible DerivedData locations, even if they don't exist yet
  derived_data_base = File.join(Dir.home, 'Library', 'Developer', 'Xcode', 'DerivedData')
  
  # Ensure DerivedData base directory exists
  FileUtils.mkdir_p(derived_data_base) rescue nil
  
  # Create in all existing Runner-* directories
  Dir.glob(File.join(derived_data_base, 'Runner-*')).each do |runner_dir|
    uninstalled_dir = File.join(runner_dir, 'Build', 'Intermediates.noindex', 
                                'ArchiveIntermediates', 'Runner', 
                                'IntermediateBuildFilesPath', 'UninstalledProducts', 'iphoneos')
    FileUtils.mkdir_p(uninstalled_dir) rescue nil
    privacy_packages.each do |pkg|
      bundle_dir = File.join(uninstalled_dir, "#{pkg}_privacy.bundle")
      FileUtils.mkdir_p(bundle_dir) rescue nil
      manifest_file = File.join(bundle_dir, "#{pkg}_privacy")
      FileUtils.touch(manifest_file) rescue nil
      FileUtils.chmod(0644, manifest_file) rescue nil
      puts "  Created in DerivedData: #{manifest_file}"
    end
  end
  
  # CRITICAL: Also create a template structure that Xcode can use
  # This ensures files exist even if DerivedData directory structure is created during build
  # Create in a common location that Xcode might check
  template_uninstalled_dir = File.join(derived_data_base, 'Runner-template', 'Build', 'Intermediates.noindex', 
                                       'ArchiveIntermediates', 'Runner', 
                                       'IntermediateBuildFilesPath', 'UninstalledProducts', 'iphoneos')
  FileUtils.mkdir_p(template_uninstalled_dir) rescue nil
  privacy_packages.each do |pkg|
    bundle_dir = File.join(template_uninstalled_dir, "#{pkg}_privacy.bundle")
    FileUtils.mkdir_p(bundle_dir) rescue nil
    manifest_file = File.join(bundle_dir, "#{pkg}_privacy")
    FileUtils.touch(manifest_file) rescue nil
    FileUtils.chmod(0644, manifest_file) rescue nil
    puts "  Created in template: #{manifest_file}"
  end
    
    # Also create a script that will be called during build to create files in new DerivedData dirs
    create_script = File.join(File.dirname(__FILE__), 'create_privacy_manifests_during_build.sh')
    File.write(create_script, <<-SCRIPT)
#!/bin/bash
# This script creates privacy manifests during Xcode build
DERIVED_DATA_BASE="$HOME/Library/Developer/Xcode/DerivedData"
PACKAGES="url_launcher_ios sqflite_darwin shared_preferences_foundation path_provider_foundation flutter_secure_storage flutter_local_notifications share_plus"

for runner_dir in "$DERIVED_DATA_BASE"/Runner-*; do
  if [ -d "$runner_dir" ]; then
    uninstalled_dir="${runner_dir}/Build/Intermediates.noindex/ArchiveIntermediates/Runner/IntermediateBuildFilesPath/UninstalledProducts/iphoneos"
    mkdir -p "$uninstalled_dir"
    for pkg in $PACKAGES; do
      BUNDLE_DIR="${uninstalled_dir}/${pkg}_privacy.bundle"
      mkdir -p "$BUNDLE_DIR"
      touch "${BUNDLE_DIR}/${pkg}_privacy"
      chmod 644 "${BUNDLE_DIR}/${pkg}_privacy"
    done
  fi
done
    SCRIPT
    FileUtils.chmod(0755, create_script) rescue nil
    puts "  Created build-time script: #{create_script}"
  end
  
  # Create a script that will create files during build in DerivedData
  # This script will be called by the script phases
  script_content = <<-SCRIPT
#!/bin/bash
# Create privacy manifest files in DerivedData during build
PACKAGES="url_launcher_ios sqflite_darwin shared_preferences_foundation path_provider_foundation flutter_secure_storage flutter_local_notifications share_plus"

# Function to create manifest for a package
create_manifest() {
  local pkg=$1
  local base_dir=$2
  
  if [ -z "$pkg" ] || [ -z "$base_dir" ]; then
    return
  fi
  
  BUNDLE_DIR="${base_dir}/${pkg}_privacy.bundle"
  mkdir -p "$BUNDLE_DIR"
  touch "${BUNDLE_DIR}/${pkg}_privacy"
  chmod 644 "${BUNDLE_DIR}/${pkg}_privacy"
  echo "Created: ${BUNDLE_DIR}/${pkg}_privacy"
}

# Try UNINSTALLED_PRODUCTS_DIR first (for archive builds)
if [ -n "${UNINSTALLED_PRODUCTS_DIR}" ]; then
  for pkg in $PACKAGES; do
    create_manifest "$pkg" "${UNINSTALLED_PRODUCTS_DIR}"
  done
fi

# Try BUILT_PRODUCTS_DIR (for regular builds)
if [ -n "${BUILT_PRODUCTS_DIR}" ]; then
  for pkg in $PACKAGES; do
    create_manifest "$pkg" "${BUILT_PRODUCTS_DIR}"
  done
fi

# Also try to find DerivedData and create there
DERIVED_DATA_BASE="$HOME/Library/Developer/Xcode/DerivedData"
if [ -d "$DERIVED_DATA_BASE" ]; then
  # Find all Runner-* directories
  for derived_dir in "$DERIVED_DATA_BASE"/Runner-*/Build/Intermediates.noindex/ArchiveIntermediates/Runner/IntermediateBuildFilesPath/UninstalledProducts/iphoneos; do
    if [ -d "$(dirname "$derived_dir")" ]; then
      mkdir -p "$derived_dir"
      for pkg in $PACKAGES; do
        create_manifest "$pkg" "$derived_dir"
      done
    fi
  done
fi
SCRIPT

  # Save script to a file that can be executed
  script_file = File.join(File.dirname(__FILE__), 'create_privacy_manifests.sh')
  File.write(script_file, script_content)
  FileUtils.chmod(0755, script_file)
  puts "  Created script: #{script_file}"
  
  # Also try to create in DerivedData if it exists (for immediate use)
  derived_data_base = File.join(Dir.home, 'Library', 'Developer', 'Xcode', 'DerivedData')
  if File.directory?(derived_data_base)
    Dir.glob(File.join(derived_data_base, 'Runner-*')).each do |derived_dir|
      uninstalled_dir = File.join(derived_dir, 'Build', 'Intermediates.noindex', 
                                  'ArchiveIntermediates', 'Runner', 
                                  'IntermediateBuildFilesPath', 'UninstalledProducts', 'iphoneos')
      if File.directory?(File.dirname(uninstalled_dir))
        FileUtils.mkdir_p(uninstalled_dir) rescue nil
        privacy_packages.each do |pkg|
          bundle_dir = File.join(uninstalled_dir, "#{pkg}_privacy.bundle")
          FileUtils.mkdir_p(bundle_dir) rescue nil
          manifest_file = File.join(bundle_dir, "#{pkg}_privacy")
          FileUtils.touch(manifest_file) rescue nil
          FileUtils.chmod(0644, manifest_file) rescue nil
        end
      end
    end
  end
end

